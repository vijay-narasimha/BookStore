<%- include('header') %>
<h1 class="booktabletitle">Books Information</h1>
<div class="booktable">

<table class="booktable">
    <tr>
        <th>S.no</th>
        <th>Book Image</th>
        <th>Book name</th>
        <th>NO of books</th>
        <th>NO of users taken</th>
    </tr>
<% books.forEach((book,index)=>{  %>

<tr>
    <td><%= index+1 %></td>
    <td><img width='100' height='100' src=<%=book.image %></td>
    <td><%= book.name %></td>
    <td><%= book.NoofBooks %></td>
    <td><%= book.issued %> </td>
</tr>
<% }) %>


</table>

</div>



<%- include('footer') %>